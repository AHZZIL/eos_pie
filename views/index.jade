      }
      to {
      -webkit-transform: translateX(0%) rotate(360deg);
      transform: translateX(0%) rotate(360deg);
      }
      }
      @keyframes rotate-translate {
      from {
      -webkit-transform: translateX(-100%) rotate(0deg);
      transform: translateX(-100%) rotate(0deg);
      }
